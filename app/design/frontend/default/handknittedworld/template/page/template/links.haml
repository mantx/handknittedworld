- $_links = $this->getLinks()
- if(count($_links)>0)
  %ul.links(id="#{$this->getName() ? $this->getName() : NULL}")
    - foreach($_links as $_link)
      - if($_link instanceof Mage_Core_Block_Abstract)
        - echo $_link->toHtml()
      - else
        %li(class="#{$_link->getIsFirst() ? first : ($_link->getIsLast() ? last : '')}" "#{$_link->getLiParams()}")
          - echo $_link->getBeforeText()
          %a(href="#{$_link->getUrl()}" title="#{$_link->getTitle()}" "#{$_link->getAParams()}")
            = $_link->getLabel()
          - echo $_link->getAfterText()

